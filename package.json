{
  "name": "catppuccin-whiskers-js",
  "version": "2.3.0",
  "description": "Modified Handlebars for creating Catppuccin color themes. Based on the whiskers tool in the catppuccin toolbox",
  "exports": {
    "import": "./index.js"
  },
  "main": "index.js",
  "scripts": {
    "prepare": "tsc && chmod +x dist/cli/whiskers-cli.js",
    "publish-scoped": "./publish-scoped.sh"
  },
  "keywords": [
    "catppuccin",
    "handlebars"
  ],
  "author": "Albert Zhang <alchzh@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@catppuccin/palette": "^0.2.0",
    "better-title-case": "^2.1.0",
    "colord": "^2.9.3",
    "front-matter": "^4.0.2",
    "handlebars": "^4.7.8",
    "minimist": "^1.2.8",
    "source-map": "^0.7.4",
    "ts-replace-all": "^1.0.0",
    "wordwrap": "^1.0.0"
  },
  "optionalDependencies": {
    "uglify-js": "^3.1.4"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "bin": {
    "whiskers-js": "cli/whiskers-cli.js"
  }
